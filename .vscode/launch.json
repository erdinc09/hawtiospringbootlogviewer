{
    "configurations": [
        {
            "type": "java",
            "name": "LogViewerTesterApplication",
            "request": "launch",
            "mainClass": "io.github.erdinc09.springboot.hawtio.logviewertester.LogViewerTesterApplication",
            "projectName": "logviewertester"
        },
        {
            "type": "java",
            "name": "Spring Boot-LogviewertesterApplication<logviewertester>",
            "request": "launch",
            "cwd": "${workspaceFolder}",
            "mainClass": "io.github.erdinc09.springboot.hawtio.logviewertester.LogviewertesterApplication",
            "projectName": "logviewertester",
            "args": "",
            "envFile": "${workspaceFolder}/.env",
            "vmArgs": " -Dcom.sun.management.jmxremote -Dcom.sun.management.jmxremote.port=64475 -Dcom.sun.management.jmxremote.authenticate=false -Dcom.sun.management.jmxremote.ssl=false -Dspring.jmx.enabled=true -Djava.rmi.server.hostname=localhost -Dspring.application.admin.enabled=true -Dspring.boot.project.name=logviewertester"
        }
    ]
}